SINGULARITY_CUSTOM \s*user=%{USERNAME_PAT} image=%{DATA:imagepath} jobid=%{DATA:jobid} cmd=%{GREEDYDATA:cmd}

SINGULARITY_COMMON \s*%{DATA:executable} \(U=%{POSINT:uid},P=%{POSINT:pid}\)>
SINGULARITY_GENERAL \s*%{SINGULARITY_COMMON} %{GREEDYDATA:singularity_msg}
SINGULARITY_CMD \s*%{SINGULARITY_COMMON} USER=%{USERNAME_PAT}, IMAGE='%{DATA:image}', COMMAND='%{DATA:cmd}'

SINGULARITY_MSG (?:%{SINGULARITY_CUSTOM}|%{SINGULARITY_CMD}|%{SINGULARITY_GENERAL})
